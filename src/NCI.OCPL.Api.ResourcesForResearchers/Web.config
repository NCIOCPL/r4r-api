<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.webServer>
    <handlers>
      <add name="aspNetCore" path="*" verb="*" modules="AspNetCoreModuleV2" resourceType="Unspecified" />
    </handlers>
    <aspNetCore processPath="dotnet" arguments=".\NCI.OCPL.Api.ResourcesForResearchers.dll" stdoutLogEnabled="false" stdoutLogFile=".\logs\stdout" />
    <security>
      <requestFiltering removeServerHeader="true">
        <fileExtensions allowUnlisted="true" />
      </requestFiltering>
    </security>
    <httpProtocol>
      <customHeaders>
        <remove name="X-Powered-By" />
      </customHeaders>
    </httpProtocol>
  </system.webServer>
</configuration>